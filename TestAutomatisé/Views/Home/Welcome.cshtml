@model TestAutomatisé.DomaineClasses.Personne

@{
    ViewBag.Title = "Home Page";
    Layout = " ~/Views/Shared/_Layout.cshtml";
}

@Scripts.Render("~/Scripts/w3.js")

<div class="w3-row-padding w3-margin-bottom w3-cell-middle nav-divider-fixed">
    <div class="w3-quarter ">
        <div class="w3-container w3-red w3-padding-16 w3-hover-shadow">
            <div class="w3-left"><i class="fa fa-comment w3-xxxlarge"></i></div>
            <div class="w3-right">
                <h3>52</h3>
            </div>
            <div class="w3-clear"></div>
            <h4>Messages</h4>
        </div>
    </div>
    <div class="w3-quarter">
        <div class="w3-container w3-blue w3-padding-16 w3-hover-shadow">
            <div class="w3-left"><i class="fa fa-spinner w3-spin" style="font-size:54px"></i></div>
            <div class="w3-right">
                <h3>99</h3>
            </div>
            <div class="w3-clear"></div>
            <h4>Voir</h4>
        </div>
    </div>
    <div class="w3-quarter">
        <div class="w3-container w3-teal w3-padding-16 w3-hover-shadow">
            <div class="w3-left"><i class="fa fa-file w3-xxxlarge"></i></div>
            <div class="w3-right">
                <h3>23</h3>
            </div>
            <div class="w3-clear"></div>
            <h4>Rapports</h4>
        </div>
    </div>
    <div class="w3-quarter">
        <div class="w3-container w3-orange w3-text-white w3-padding-16 w3-hover-shadow
                         ">
            <div class="w3-left"><i class="fa fa-users w3-xxxlarge"></i></div>
            <div class="w3-right">
                <h3>50</h3>
            </div>
            <div class="w3-clear"></div>
            <h4>Utilisateurs</h4>
        </div>
    </div>
</div>
<div class="w3-panel">
    <div class="w3-row-padding" style="margin:0 -16px">

        <div>
            <h5>Feeds</h5>
            <table class="w3-table w3-striped w3-white tab">
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-user w3-text-blue w3-large "></i></td>
                    <td>New record, over 90 views.</td>
                    <td><i>10 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-bell w3-text-red w3-large "></i></td>
                    <td>Database error.</td>
                    <td><i>15 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-users w3-text-yellow w3-large "></i></td>
                    <td>New record, over 40 users.</td>
                    <td><i>17 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-comment w3-text-red w3-large"></i></td>
                    <td>New comments.</td>
                    <td><i>25 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-bookmark w3-text-blue w3-large"></i></td>
                    <td>Check transactions.</td>
                    <td><i>28 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-laptop w3-text-red w3-large"></i></td>
                    <td>CPU overload.</td>
                    <td><i>35 mins</i></td>
                </tr>
                <tr class="w3-hover-dark-grey">
                    <td><i class="fa fa-share-alt w3-text-green w3-large"></i></td>
                    <td>New shares.</td>
                    <td><i>39 mins</i></td>
                </tr>
            </table>
        </div>
    </div>
</div>
<hr>
<div class="w3-container">
    <h5 class="w3-text-white">General Stats</h5>
    <p class="w3-text-white">New Visitors</p>
    <div class="w3-grey">
        <div class="w3-container w3-center w3-padding w3-green" style="width:25%">+25%</div>
    </div>

    <p class="w3-text-white">New Users</p>
    <div class="w3-grey">
        <div class="w3-container w3-center w3-padding w3-orange" style="width:50%">50%</div>
    </div>

    <p class="w3-text-white">Bounce Rate</p>
    <div class="w3-grey">
        <div class="w3-container w3-center w3-padding w3-red" style="width:75%">75%</div>
    </div>
</div>


<div class="w3-container ">
    <h5 class="w3-text-white">Recent Users</h5>
    <ul class="w3-ul w3-card-4 w3-white w3-hoverable" id="myUL">
        <li class="w3-padding-16">
            <span onclick="this.parentElement.style.display='none'"
                  class="w3-bar-item w3-button w3-xlarge w3-right">&times;</span>
            <img src="~/Images/francis-turbo-portrait.jpg" class="w3-left w3-circle w3-margin-right" style="width:40px">
            <span class="w3-xlarge">Francis Turbo</span><br>
        </li>
        <li class="w3-padding-16">
            <span onclick="this.parentElement.style.display='none'"
                  class="w3-bar-item w3-button w3-xlarge w3-right">&times;</span>
            <img src="~/Images/21827091_122528671802397_323022888309358592_n.jpg" class="w3-left w3-circle w3-margin-right" style="width:40px;height:40px">
            <span class="w3-xlarge">Karen</span><br>
        </li>
        <li class="w3-padding-16">
            <span onclick="this.parentElement.style.display='none'"
                  class="w3-bar-item w3-button w3-xlarge w3-right">&times;</span>
            <img src="~/Images/LOVA-NANTENIANA.jpg" class="w3-left w3-circle w3-margin-right" style="width:40px;height:40px">
            <span class="w3-xlarge">Lova Nantenaina</span><br>
        </li>
    </ul>
</div>
<hr>

<div class="w3-container">
    <h5 class="w3-text-white">Recent Comments</h5>
    <div class="w3-row">
        <div class="w3-col m2 text-center">

            <img class="w3-circle" alt="Norway" src="~/Images/portrait-entreprise-manuel-salazar-4.jpg" style="width:80px;height:80px">
        </div>
        <div class="w3-col m10 w3-container">
            <span onclick="this.parentElement.style.display='none'"
                  class="w3-bar-item w3-button w3-xxlarge w3-right w3-hover-text-white">&times;</span>
            <h4 class="w3-text-white">John <span class="w3-opacity w3-medium  w3-text-white">Sep 29, 2014, 9:12 PM</span></h4>
            <p class="w3-text-white">On peut classifier le test comme fait</p><br>
        </div>

    </div>

    <div class="w3-row">
        <div class="w3-col m2 text-center">

            <img class="w3-circle" alt="Norway" src="~/Images/stacy-martin.jpg" style="width:80px;height:80px">
        </div>

        <div class="w3-col m10 w3-container">
            <span onclick="this.parentElement.style.display='none'"
                  class="w3-bar-item w3-button w3-xxlarge w3-right">&times;</span>
            <h4 class="w3-text-white">Debora <span class="w3-opacity w3-medium w3-text-white">Sep 28, 2014, 10:15 PM</span></h4>
            <p class="w3-text-white">Sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p><br>
        </div>

    </div>
</div>
<br>
<div class="w3-container w3-dark-grey w3-padding-32">
    <div class="w3-row">
        <div class="w3-container w3-third">
            <h5 class="w3-bottombar w3-border-green">Demographic</h5>
            <p>Language</p>
            <p>Country</p>
            <p>City</p>
        </div>
        <div class="w3-container w3-third">
            <h5 class="w3-bottombar w3-border-red">System</h5>
            <p>Browser</p>
            <p>OS</p>
            <p>More</p>
        </div>
        <div class="w3-container w3-third">
            <h5 class="w3-bottombar w3-border-orange">Target</h5>
            <p>Users</p>
            <p>Active</p>
            <p>Geo</p>
            <p>Interests</p>
        </div>
    </div>
</div>

<footer class="w3-container w3-padding-small">
    <a class="w3-btn w3-xlarge w3-circle w3-right w3-blue" style="position:relative;right:16px;text-decoration:none;z-index:1;"><i class="fa fa-envelope "></i></a>
    <h4 class="w3-text-white">Orange Madagascar</h4>
    <p class="w3-text-white">Powered by <i><span class="w3-opacity w3-text-white"> Vavaka Ratafika Niry Zo Imanoela</span></i></p>
</footer>





