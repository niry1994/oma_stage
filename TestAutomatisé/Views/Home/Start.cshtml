
@{
    ViewBag.Title = "Start";
}
<div class="preload ">
    <div class="logo w3-animate-top">Test-<span class="automa">Automatisé</span></div>
    <div class="loader-frame">
        <div class="loader1" id="loader1"></div>
        <div class="loader2" id="loader2"></div>
    </div>
</div>

<script>
    (function(){
        var preload= document.getElementById("preload");
        var loading = 0;
        var id = setInterval(frame, 5);
        function frame(){
            if(loading == 100){
                clearInterval(id);
                window.open("/Home/login","_self");
            
            } else{
                loading=loading + 1;
       
            }
            if (loading == 90) {
                preload.style.animation = "fadeout is ease";

            }
        
        }
    })();
</script>